<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Dictionary">
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.droft.prototype.dictionary.DictionaryDm"/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <entry-point class='org.droft.prototype.dictionary.client.Dictionary'/>
    <set-property name="user.agent" value="safari"/>
    <source path='client' />
    <source path="components"></source>
    <inherits name='org.gwtbootstrap3.GwtBootstrap3'/>
    <!--For SDM-->
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <!--bt configuration-->
    <inherits name="org.gwtbootstrap3.extras.respond.Respond"/>
    <!--GIN-->
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.logging.Logging"/>

    <!--dev-->
    <set-configuration-property name="gwt.superdevmode" value="on" />

</module>